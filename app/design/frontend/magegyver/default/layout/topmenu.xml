<layout version="0.1.0">
	<!-- Don't forget to put this file to your design or dereference it in
	     app/code/local/MageGyver/Topmenu/etc/config.xml at
	     config>frontend>layout>updates -->
	<default>
		<reference name="catalog.topnav">
			<action method="addLink" translate="label">
				<label>About Us</label>
				<type>path</type> <!-- currently only "path" is supported -->
				<path>about-magento-demo-store/</path>
			</action>
		</reference>
	</default>
</layout>